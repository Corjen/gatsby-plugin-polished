{
  "name": "gatsby-plugin-polished",
  "description": "Gatsby plugin to add polished babel plugin",
  "version": "1.0.4",
  "author": "Corjen Moll <corjen.moll@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/Corjen/gatsby-plugin-polished",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
