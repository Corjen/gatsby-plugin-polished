    "babel-preset-gatsby-package": "^0.1.4"
  },
  "peerDependencies": {
    "babel-plugin-polished": "^1.0.0"
  },
  "bugs": {
    "url": "https://github.com/Corjen/gatsby-plugin-polished/issues"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "polished"
  ]
}
